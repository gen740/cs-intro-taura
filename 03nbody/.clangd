CompileFlags:
  Add:
    - -Wall
    - -Wextra
    - -std=c++20
    - -O3
    - -DNDEBUG
    - -march=native
    - -Wall
    - -Wextra
    - -fopenmp
  CompilationDatabase: ./build

Diagnostics:
  UnusedIncludes: Strict

InlayHints:
  Enabled: Yes
  ParameterNames: Yes
  DeducedTypes: Yes

Hover:
  ShowAKA: Yes


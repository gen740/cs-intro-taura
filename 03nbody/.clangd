    - -O3
    - -DNDEBUG
    - -march=native
    - -Wall
    - -Wextra
    - -fopenmp
    # - -nostdinc++
    # - -I/usr/lib/gcc/x86_64-pc-linux-gnu/13.1.1/../../../../include/c++/13.1.1
